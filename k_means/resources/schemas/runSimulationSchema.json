{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "NumberClusters": {
      "type": "integer",
      "minimum": 1
    },
    "NumberDistributions": {
      "type": "integer",
      "minimum": 1
    },
    "NumberSamples": {
      "type": "integer",
      "minimum": 1
    },
    "EpsilonForConvergence": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    },
    "MaxIterations": {
      "type": "integer",
      "minimum": 1
    },
    "AddNoise": {
      "type": "boolean"
    },
    "PauseLength": {
      "type": "number",
      "minimum": 0,
      "exclusiveMinimum": true
    },
    "Seed": {
      "type": "integer",
      "minimum": 1
    }
  },
  "additionalProperties": true,
  "required": [
    "NumberClusters",
    "NumberDistributions",
    "NumberSamples",
    "EpsilonForConvergence",
    "MaxIterations",
    "AddNoise",
    "PauseLength",
    "Seed"
  ]
}